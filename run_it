#!/bin/sh

pip install --upgrade src/we_panic_utils/

screen python src/run_model.py CNN_3D picky_32_32 \
--features rr \
--csv wepanic_collated_catalogue.csv \
--output_dir run_history/p_32x32_grey_rr \
--epochs 100 \
--dimensions 32 32 \
--steps_per_epoch 500 \
--batch_size 14 \
--greyscale_on \
--kfold 4 \
--early_stopping 20 \
--loss mean_squared_error
