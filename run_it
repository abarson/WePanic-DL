#!/bin/sh

pip install --upgrade src/we_panic_utils/

screen python src/run_model.py ShallowC3D picky_16_16 \
--features hr \
--csv wepanic_collated_catalogue.csv \
--output_dir run_history/p_16_16_grey \
--epochs 30 \
--dimensions 16 16 \
--steps_per_epoch 500 \
--batch_size 14 \
--greyscale_on \
--kfold 4 \
--early_stopping 12 \
--loss mean_squared_error
