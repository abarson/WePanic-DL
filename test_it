#!/bin/sh

pip install --upgrade src/we_panic_utils/
python src/run_model.py CNN_3D picky_32_32 \
--csv wepanic_collated_catalogue.csv \
--output_dir run_history/TEST \
--features hr \
--epochs 1 \
--steps_per_epoch 10 \
--batch_size 14 \
--kfold 4 \
--epochs 100 \
--dimensions 32 32 \
--steps_per_epoch 500 \
--batch_size 14 \
--greyscale_on \
--kfold 4 \
--early_stopping 20 \
--loss mean_squared_error
