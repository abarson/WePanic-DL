#!/bin/sh

## script for runs on the cloud

pip install --upgrade src/we_panic_utils/
epochs=1
steps_per_epoch=500
batch_size=30

python src/run_model.py DualNet glued_32_32 \
--features hr \
--csv wepanic_collated_catalogue3.csv \
--output_dir run_history/TEST_DUALNET \
--epochs $epochs \
--dimensions 32 32 \
--steps_per_epoch $steps_per_epoch \
--batch_size $batch_size \
--kfold 3 \
--early_stopping 10 \
--loss mean_squared_error \
--sequence_length 90 
