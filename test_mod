#!/bin/sh

N=$1

pip install --upgrade src/we_panic_utils/
python src/run_model.py CNN_3D picky_32_32 \
--csv wepanic_collated_catalogue.csv \
--input_dir run_history/final_sim_redscale_on_hr \
--qbc run_history/final_sim_redscale_on_hr/models \
--features hr \
--steps_per_epoch 500 \
--batch_size 14 \
--epochs 1 \
--sequence_length 60 \
--redscale_on
