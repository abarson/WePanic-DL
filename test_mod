#!/bin/sh

pip install --upgrade src/we_panic_utils/
python src/run_model.py BN_CNN_3D_DO glued_32_32 \
--csv WPCC_new-dist.csv \
--input_dir committee_top2 \
--qbc committee_top2 \
--features rr \
--steps_per_epoch 500 \
--batch_size 1 \
--num_val_clips 25 \
--epochs 1 \
--sequence_length 90 \
--redscale_on
